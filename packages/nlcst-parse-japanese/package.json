{
  "name": "nlcst-parse-japanese",
  "version": "1.2.2",
  "description": "Parse Japanese text and output as NLCST.",
  "keywords": [
    "NLCST",
    "parse",
    "parser",
    "japanese"
  ],
  "homepage": "https://github.com/azu/nlp-pattern-match/tree/master/packages/nlcst-parse-japanese/",
  "bugs": {
    "url": "https://github.com/azu/nlp-pattern-match/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/nlp-pattern-match.git"
  },
  "license": "MIT",
  "author": "azu",
  "main": "lib/nlcst-parse-japanese.js",
  "types": "lib/nlcst-parse-japanese.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css}'",
    "prepublish": "npm run --if-present build",
    "test": "mocha 'test/**/*.ts'",
    "watch": "tsc -p . --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "kuromojin": "^1.4.0",
    "nlcst-types": "^1.2.4",
    "unist-types": "^1.1.5"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.19",
    "cross-env": "^7.0.3",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
