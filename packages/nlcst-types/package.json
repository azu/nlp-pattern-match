    "test": "mocha 'test/**/*.ts'",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "unist-types": "^1.1.6"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.19",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
